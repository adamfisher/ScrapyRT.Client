<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>An SDK client to make calls to a scrapyrt http endpoint.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>adamfisher</Authors>
    <Company>adamfisher</Company>
    <Copyright />
    <PackageProjectUrl></PackageProjectUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/adamfisher/ScrapyRT.Client/master/logo.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/adamfisher/ScrapyRT.Client</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>scrapy, scrapy crawler, scrapy spider, scrapy framework, crawler, scraper</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
